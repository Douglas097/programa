i,j: inteiro

inicio
// inicio do comando

para i de 1 ate 3 faca
     para j de 1 ate 3 faca
          escreva("Digite os numeros: [",i, " + ", j, " ] ")
          leia(matrizA[i,j])
     fimpara
fimpara

para i de 1 ate 3 faca
     para j de 1 ate 3 faca
          escreva(matrizA[i,j])
     fimpara
     escreval("")
fimpara

