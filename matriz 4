algoritmo "atividade5"
var
vet:vetor[1..7,1..8] de inteiro
i,j:inteiro

inicio
// inicio do comando

para i de 1 ate 7 faca
     para j de 1 ate 8 faca
          vet[i,j]<-i+i
     fimpara
fimpara

para i de 1 ate 7 faca
     para j de 1 ate 8 faca
          escreva(vet[i,j]," ")
     fimpara
          escreval(" ")
fimpara

fimalgoritmo
